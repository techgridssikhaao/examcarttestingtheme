<div class="container">
  {% include 'breadcrumb' %} 
  <div class="row">
    <div id="content" class="col-sm-12">
      <h1>{{ page.title }}</h1>
      <div class="rte">
        {{ page.content }}
      </div>
    </div>
  </div>
</div>
{% schema %}
  {
    "name": "Page",
    "settings": []
  }
{% endschema %}

<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "WebSite",
  "name": "Agrawal Examcart",
  "alternateName": "Examcart",
  "url": "https://examcart.in",
  "potentialAction": {
    "@type": "SearchAction",
    "target": {
      "@type": "EntryPoint",
      "urlTemplate": "https://examcart.in/search?type=product&q=(search_term_string)",
      "query-input": "required name=search_term_string"
    }
  }
}
</script>
